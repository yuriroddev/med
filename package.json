{
  "name": "save-icu",
  "version": "1.0.0",
  "description": "COVID-19 project",
  "scripts": {
    "start": "npm-run-all --parallel dev:del dev:webpack dev:sass-dev dev:eleventy",
    "dev:del": "rimraf dist",
    "dev:sass-dev": "node-sass --watch _process/scss/style.scss --output-style expanded --source-map true --output _site/css",
    "dev:eleventy": "eleventy --serve --quiet",
    "dev:webpack": "webpack --config webpack.dev.js --watch",
    "build": "run-s prod:*",
    "prod:del": "rimraf ../buildMed",
    "prod:sass-dev": "node-sass _process/scss/style.scss --output-style compressed --output ../buildMed/css",
    "prod:webpack": "webpack --config webpack.prod.js --env.production --silent -p --optimize-minimize",
    "prod:serve": "eleventy --output=../buildMed",
    "prod:copy-gitignore": "cpy ../.gitignore ../buildMed"
  },
  "author": "Yuri",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.7.1",
    "@11ty/eleventy-plugin-rss": "^1.0.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.1",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.0",
    "favicons-webpack-plugin": "^3.0.1",
    "luxon": "^1.28.1",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^5.0.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.14.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.5.1",
    "vue": "^2.6.6"
  }
}
